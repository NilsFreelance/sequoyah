<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="qemuarmDevice"
         name="qemuarmDevice"
         point="org.eclipse.tml.device">
      <device
            copyright="Motorola Inc. 2007"
            description="Mobile Emulator for QEMUARM"
            handler="org.eclipse.tml.device.qemuarm.handler.QEmuARMDeviceHandler"
            icon="icons/full/obj16/qemuarm.gif"
            id="org.eclipse.tml.device.qemuarm.qemuarmDevice"
            name="QEmuARM Mobile Device"
            provider="Eclipse.org"
            version="0.2.0">
      </device>
   </extension>
   <extension
         id="org.eclipse.tml.device.qemuarm.qemuarmDevice"
         name="startService"
         point="org.eclipse.tml.serviceDefinition">
      <service
            id="org.eclipse.tml.service.start.startService">
         <status
               endId="IDLE"
               haltId="OFF"
               handler="org.eclipse.tml.device.qemuarm.handler.StartStatusHandler"
               startId="OFF">
         </status>
      </service>
   </extension>
   <extension
         id="org.eclipse.tml.device.qemuarm.qemuarmDevice"
         name="stopService"
         point="org.eclipse.tml.serviceDefinition">
      <service
            id="org.eclipse.tml.service.stop.stopService">
         <status
               endId="OFF"
               haltId="IDLE"
               handler="org.eclipse.tml.device.qemuarm.handler.StopStatusHandler"
               startId="IDLE">
         </status>
         <status
               endId="OFF"
               haltId="IDLE-VNC"
               handler="org.eclipse.tml.device.qemuarm.handler.StopStatusHandler"
               startId="IDLE-VNC">
         </status>
      </service>
   </extension>
   <extension

          id="org.eclipse.tml.device.qemuarm.qemuarmDevice"

          name="vncViewerService"

          point="org.eclipse.tml.serviceDefinition">

       <service

             id="org.eclipse.tml.service.vncviewer.vncViewerService">

          <status

                endId="IDLE-VNC"

                haltId="IDLE"

                handler="org.eclipse.tml.device.qemuarm.handler.VNCStatusHandler"

                startId="IDLE">

          </status>
          <status
                endId="IDLE-VNC"
                haltId="IDLE-VNC"
                handler="org.eclipse.tml.device.qemuarm.handler.VNCStatusHandler"
                startId="IDLE-VNC">
          </status></service>

    </extension>
   <extension
         id="org.eclipse.tml.device.qemuarm.qemuarmDevice"
         name="unplugVncViewerService"
         point="org.eclipse.tml.serviceDefinition">
      <service
            id="org.eclipse.tml.service.vncviewer.unplugVncViewerService">
         <status
               endId="IDLE"
               haltId="IDLE-VNC"
               handler="org.eclipse.tml.device.qemuarm.handler.UnplugVNCStatusHandler"
               startId="IDLE-VNC">
         </status>
      </service>
   </extension>
   <extension
         id="IDLE-VNC"
         point="org.eclipse.tml.status">
      <status
            id="IDLE-VNC"
            image="icons/full/obj16/qemuarm.gif"
            name="IDLE-VNC">
      </status>
   </extension>

</plugin>
